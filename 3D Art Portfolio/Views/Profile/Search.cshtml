@model IEnumerable<_3D_Art_Portfolio.Models.ApplicationUser>
@using _3D_Art_Portfolio.Models

@{
    ViewBag.Title = "Search";
}

<tbody>
    @if(@Model.Count()>0)
    {
        foreach (ApplicationUser a in @Model)
        {
            <tr>
                <td><img class="otherImages" alt="@a.UserName" src=@Url.Content(a.ProfilePicture) /></td>
                <td><a href=@String.Format("/Profile/ViewProfile/"+ @a.Id)>@a.UserName</a></td>
            </tr>
        }
    }
    else
    {
        <tr><td>No results :(</td></tr>
    }
</tbody>

